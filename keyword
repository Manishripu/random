import com.fasterxml.jackson.databind.JsonNode;
import com.networknt.schema.*;

public class EndGreaterThanStartKeyword extends AbstractKeyword {

    public static final String NAME = "endGreaterThanStart";

    public EndGreaterThanStartKeyword() {
        super(NAME);
    }

    @Override
    public KeywordValidator newValidator(
            SchemaLocation schemaLocation,
            JsonNode schemaNode,
            JsonSchema parentSchema,
            ValidationContext validationContext) {

        return new EndGreaterThanStartValidator(schemaLocation);
    }
}